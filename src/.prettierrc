{
  "tabWidth": 2,
  "useTabs": false,
  "endOfLine": "lf",
  "trailingComma": "es5",
  "printWidth": 120,
  "semi": false,
  "singleQuote": false,
  "overrides": [
    {
      "files": "*.vue",
      "options": {
        "htmlWhitespaceSensitivity": "ignore",
        "vueIndentScriptAndStyle": false,
        "singleAttributePerLine": false
      }
    }
  ]
}
